@{
    ViewData["Title"] = "Welcome";
}

<h2>Welcome</h2>

<ul>
    <!-- Sintaxis razor para agregar codigo C# en HTML -->
    <!-- Ciclo for que me imprime el mensaje Hello la cantidad de veces que se especifique en la variable NumTimes -->
    @for (int i = 0; i < (int)ViewData["NumTimes"]!; i++)
    {
        <li>@ViewData["Message"]</li>
    }
</ul>
